{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/shorturl/([0-9]+)",
      "methods": ["GET"],
      "dest": "api/shorturl/[id].js"
    },
    {
      "src": "/api/shorturl",
      "methods": ["POST"],
      "dest": "api/shorturl.js"
    }
  ]
}


